<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<sbe:messageSchema xmlns:sbe="http://www.fixprotocol.org/ns/simple/1.0"
                   package="uk.co.real_logic.fix_gateway.messages"
                   id="666"
                   version="0"
                   semanticVersion="0.1"
                   description="Internal messaging format used by the FIX Gateway"
                   byteOrder="littleEndian">
    <types>
        <composite name="messageHeader" description="Message identifiers and length of message root">
            <type name="blockLength" primitiveType="uint16"/>
            <type name="templateId" primitiveType="uint16"/>
            <type name="schemaId" primitiveType="uint16"/>
            <type name="version" primitiveType="uint16"/>
        </composite>
        <composite name="varDataEncoding">
            <type name="length" primitiveType="uint16"/>
            <type name="varData" primitiveType="uint8" length="0"/>
        </composite>
    </types>
    <types>
        <type name="FixSessionId" primitiveType="int64"/>
        <type name="LibraryId" primitiveType="uint16"/>
        <type name="ConnectionId" primitiveType="int64"/>
        <type name="TermId" primitiveType="int32"/>
        <type name="MessageType" primitiveType="int32"/>
        <type name="Timestamp" primitiveType="int64"/>
        <enum name="ConnectionType" encodingType="uint8">
            <validValue name="ACCEPTOR">0</validValue>
            <validValue name="INITIATOR">1</validValue>
        </enum>
        <enum name="GatewayError" encodingType="uint16">
            <validValue name="EXCEPTION">0</validValue>
            <validValue name="DUPLICATE_SESSION">1</validValue>
            <validValue name="UNABLE_TO_CONNECT">2</validValue>
            <validValue name="UNKNOWN_LIBRARY">3</validValue>
            <validValue name="DUPLICATE_LIBRARY_ID">4</validValue>
            <validValue name="DUPLICATE_ACCEPTOR">5</validValue>
        </enum>
        <enum name="MessageStatus" encodingType="uint8">
            <validValue name="OK">0</validValue>
            <validValue name="INVALID_CHECKSUM">1</validValue>
            <validValue name="INVALID_BODYLENGTH">2</validValue>
            <validValue name="INVALID">3</validValue>
        </enum>
        <enum name="DisconnectReason" encodingType="uint8">
            <validValue name="REMOTE_DISCONNECT"
                description="The TCP connection was disconnected remotely">0</validValue>
            <validValue name="LOCAL_DISCONNECT"
                description="We are disconnecting the connection due to session management rules">1</validValue>
            <validValue name="LIBRARY_DISCONNECT"
                description="We are disconnecting the connection due to its library disconnecting">2</validValue>
            <validValue name="ENGINE_SHUTDOWN"
                description="We are disconnecting the connection due to the engine shutting down">3</validValue>
        </enum>

        <enum name="AcknowledgementStatus" encodingType="uint8">
            <validValue name="OK">0</validValue>
            <validValue name="WRONG_TERM">1</validValue>
            <validValue name="MISSING_LOG_ENTRIES">2</validValue>
        </enum>

        <enum name="Vote" encodingType="uint8">
            <validValue name="FOR">0</validValue>
            <validValue name="AGAINST">1</validValue>
        </enum>

    </types>

    <!-- MESSAGING PROTOCOL -->

    <sbe:message name="FixMessage" id="1" description="A framed FIX message">
        <field name="libraryId" id="41" type="LibraryId"/>
        <field name="messageType" id="1" type="MessageType"/>
        <field name="session" id="2" type="FixSessionId"/>
        <field name="connection" id="3" type="ConnectionId"/>
        <field name="timestamp" id="4" type="Timestamp"/>
        <field name="status" id="28" type="MessageStatus"/>
        <data name="body" id="5" type="varDataEncoding"/>
    </sbe:message>
    <sbe:message name="Disconnect" id="7" description="A connection has disconnected">
        <field name="libraryId" id="42" type="LibraryId"/>
        <field name="connection" id="8" type="ConnectionId"/>
        <field name="reason" id="46" type="DisconnectReason"/>
    </sbe:message>
    <sbe:message name="Logon" id="9" description="A session has connected">
        <field name="libraryId" id="43" type="LibraryId"/>
        <field name="connection" id="10" type="ConnectionId"/>
        <field name="session" id="11" type="FixSessionId"/>
    </sbe:message>
    <sbe:message name="Connect" id="10" description="A TCP Connection has connected">
        <field name="libraryId" id="19" type="LibraryId"/>
        <field name="connection" id="17" type="ConnectionId"/>
        <field name="type" id="24" type="ConnectionType"/>
        <field name="lastSequenceNumber" id="49" type="int32"/>
        <data name="address" id="18" type="varDataEncoding"/>
    </sbe:message>
    <sbe:message name="InitiateConnection" id="11" description="Try to make an outbound connection">
        <field name="libraryId" id="44" type="LibraryId"/>
        <field name="port" id="20" type="int32"/>
        <data name="host" id="21" type="varDataEncoding"/>
        <data name="senderCompId" id="22" type="varDataEncoding"/>
        <data name="senderSubId" id="38" type="varDataEncoding"/>
        <data name="senderLocationId" id="39" type="varDataEncoding"/>
        <data name="targetCompId" id="23" type="varDataEncoding"/>
    </sbe:message>
    <sbe:message name="RequestDisconnect" id="12" description="Request a connection be disconnected">
        <field name="libraryId" id="45" type="LibraryId"/>
        <field name="connection" id="24" type="ConnectionId"/>
    </sbe:message>
    <sbe:message name="Error" id="13" description="An error has occurred">
        <field name="type" id="25" type="GatewayError"/>
        <field name="libraryId" id="19" type="LibraryId"/>
        <data name="message" id="26" type="varDataEncoding"/>
    </sbe:message>
    <sbe:message name="ApplicationHeartbeat" id="16"
                 description="A heartbeat message sent within the application protocol">
        <field name="libraryId" id="40" type="LibraryId"/>
    </sbe:message>
    <sbe:message name="LibraryConnect" id="17"
                 description="Notify an engine that a new library has connected">
        <field name="libraryId" id="47" type="LibraryId"/>
        <field name="typeHandled" id="48" type="ConnectionType"/>
    </sbe:message>

    <!-- EXCEPTION STORAGE DATA STRUCTURES -->

    <sbe:message name="ExceptionEntry" id="14" description="">
        <field name="status" id="37" type="int32"/>
        <field name="hash" id="35" type="int32"/>
        <field name="time" id="27" type="int64"/>
        <field name="elementCount" id="28" type="int8"/>
        <data name="exceptionClassName" id="29" type="varDataEncoding"/>
        <data name="message" id="30" type="varDataEncoding"/>
    </sbe:message>
    <sbe:message name="StackTraceElement" id="15" description="">
        <field name="lineNumber" id="31" type="int32"/>
        <data name="className" id="32" type="varDataEncoding"/>
        <data name="methodName" id="33" type="varDataEncoding"/>
        <data name="fileName" id="34" type="varDataEncoding"/>
    </sbe:message>

    <!-- ARCHIVAL DATA STRUCTURES -->

    <sbe:message name="ReplayIndexRecord" id="5" description="Stores index information about a single record">
        <field name="streamId" id="12" type="int32"/>
        <field name="aeronSessionId" id="27" type="int32"/>
        <field name="position" id="13" type="int64"/>
        <field name="sequenceNumber" id="14" type="int32"/>
    </sbe:message>
    <sbe:message name="ArchiveMetaData" id="6" description="Meta data about 1 session's archive">
        <field name="initialTermId" id="15" type="int32"/>
        <field name="termBufferLength" id="16" type="int32"/>
    </sbe:message>

    <!-- Sequence Number Cache Structure -->
    <sbe:message name="LastKnownSequenceNumber" id="19" description="">
        <field name="sessionId" id="51" type="FixSessionId"/>
        <field name="sequenceNumber" id="52" type="int32"/>
    </sbe:message>

    <!-- Clustering/Replication Protocol -->
    <sbe:message name="MessageAcknowledgement" id="2"
                 description="Acknowledgement that you've received up to a certain position">
        <field name="newAckedPosition" id="53" type="int64"/>
        <field name="nodeId" id="54" type="int16"/>
        <field name="status" id="58" type="AcknowledgementStatus"/>
    </sbe:message>
    <sbe:message name="RequestVote" id="20" description="">
        <field name="lastAckedPosition" id="55" type="int64"/>
        <field name="candidateId" id="56" type="int16"/>
        <field name="leaderShipTerm" id="59" type="int32"/>
    </sbe:message>
    <sbe:message name="ReplyVote" id="22" description="">
        <field name="senderNodeId" id="65" type="int16"/>
        <field name="candidateId" id="62" type="int16"/>
        <field name="leaderShipTerm" id="60" type="int32"/>
        <field name="vote" id="61" type="Vote"/>
    </sbe:message>
    <sbe:message name="ConcensusHeartbeat" id="21" description="">
        <field name="nodeId" id="57" type="int16"/>
        <field name="leaderSessionId" id="27" type="int32"/>
        <field name="leaderShipTerm" id="66" type="int32"/>
        <field name="position" id="64" type="int64"/>
    </sbe:message>

</sbe:messageSchema>
